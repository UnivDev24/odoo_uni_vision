<odoo>
  <data>
      
       <record model="ir.ui.view" id="view_calendar_event_form_inherited">
            <field name="name">calendar.event.form.inherited</field>
            <field name="model">calendar.event</field>
            <field name="inherit_id" ref="calendar.view_calendar_event_form" />
            <field name="priority" eval="1"/>
            <field name="arch" type="xml">

                <xpath expr="//page[@name='page_misc']" position="replace">
                    
                     <page name="page_misc" string="Misc" groups="base.group_no_one">
                            <group>
                                <label for="user_id" string="Owner"/>
                                <field name="user_id" nolabel="1"/>
                            </group>
                        </page>
            
                </xpath>                
                
     

            </field>                               
        </record>
      
      

      
      
      
  </data>
</odoo>