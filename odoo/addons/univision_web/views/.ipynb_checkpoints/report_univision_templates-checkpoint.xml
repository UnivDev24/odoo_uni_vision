<odoo>
    <data>
         <template id="external_layout_univision" inherit_id="web.external_layout_background">
	           
       
            <xpath expr="//div[@class='header']" position="replace">
                
                 <div class="header">
                    <div class="o_background_header_univision">
                    <div class="float-right">
                        <h3 class="mt0 text-right" t-field="company.report_header"/>
                    </div>
                    <img t-if="company.logo" t-att-src="image_data_uri(company.logo)" class="float-left" alt="Logo"/>
                  
                    <div class="clearfix mb8"/>
                    </div>
                </div>                
                
            </xpath>
             
             
             <!--<xpath expr="//div[@class='float-left company_address']" position="replace">
            </xpath>-->
             
              <xpath expr="//div[@class='footer o_background_footer']/div/div[@class='text-muted']" position="replace">
                   <div class="text-muted">
                    
                    <span class="page"/>
                    /
                    <span class="topage"/>
                </div>
            </xpath>
           
        </template>
    </data>
</odoo>